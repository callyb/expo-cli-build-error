{
  "expo": {
    "name": "Bandex",
    "description": "",
    "slug": "bandex",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/logo/bandex_splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#607D8B"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.pixelistdesign.bandex",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to take photos that the user can use in the marketing they create with the app.",
        "NSFaceIDUsageDescription": "This app uses FaceID and TouchID to allow users to log in"
        }
    }
  
  }
}
